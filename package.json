  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "bem-environ": "~1.4.0",
    "glob": "^4.4.1",
    "marked": "^0.3.3",
    "mkdirp": "^0.5.0",
    "request": "^2.53.0",
    "rimraf": "^2.2.8",
    "vow": "^0.4.8"
  },
  "devDependencies": {
    "borschik": "^1.3.0",
    "borschik-tech-cleancss": "^1.0.0",
    "bower": "^1.3.12",
    "browser-sync": "^2.9.3",
    "enb": "^0.17.2",
    "enb-bem-techs": "^2.0.0",
    "enb-bemxjst": "^2.0.0",
    "enb-borschik": "^2.0.0",
    "enb-diverse-js": "^0.1.0",
    "enb-modules": "^0.2.0",
    "enb-stylus": "^2.0.0",
    "watch-run": "^1.2.2",
    "ym": "^0.1.2"
  },
  "scripts": {
    "postinstall": "[ \"$NODE_ENV\" != 'production' ] && npm run deps || true",
    "deps": "bower i",
    "start": "browser-sync start --server output-en --port 8008 --files "output-en/*.css"",
    "build": "mkdir -p raw-data && rm -rf raw-data/* && enb make && node bin/get-data.js && node bin/render.js && node bin/make.js && cp content/robots.txt output-en && cp favicon.ico output-en",
    "watch": "watch-run -i -p '{*.blocks/**,content/**,bin/**,.enb/make.js}' 'npm run build'"
  },
  "private": true
}
