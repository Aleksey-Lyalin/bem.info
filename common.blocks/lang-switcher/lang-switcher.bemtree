block('lang-switcher').replace()(function() {
    return {
        block : 'select',
        mix : 'lang-switcher',
        mods : { mode : 'radio', theme : 'islands', size : 'm' },
        name : 'lang',
        val : this.data.lang,
        options : [
            { val : 'ru', text : 'RU' },
            { val : 'en', text : 'EN' }
        ]
    };
});
